get_pokemon
    foreach
        moves.poke_moves($pokemon_name, $move_name, $attack_strategy, $attack_type)
    assert
        moves.get_pokemon($pokemon_name, $move_name, $attack_strategy, $attack_type)

get_type
    foreach
        pk_types.type_id($type_id, $pokemon_name)
    assert
        pk_types.type_name($type_id, $pokemon_name)
